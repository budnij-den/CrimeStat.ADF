{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "crimestatdafaapdev"
		},
		"crimestatdwh_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'crimestatdwh'"
		},
		"crimestatstacap_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'crimestatstacap'"
		},
		"crimestatkevalinkedservice_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://crimestatkevaapdev.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"crimestatdwh_connectionString": {
						"value": "[parameters('crimestatdwh_connectionString')]"
					},
					"crimestatstacap_connectionString": {
						"value": "[parameters('crimestatstacap_connectionString')]"
					},
					"crimestatkevalinkedservice_properties_typeProperties_baseUrl": {
						"value": "[parameters('crimestatkevalinkedservice_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}